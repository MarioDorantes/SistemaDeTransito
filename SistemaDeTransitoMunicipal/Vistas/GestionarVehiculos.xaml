<Window x:Class="SistemaDeTransitoMunicipal.Vistas.GestionarVehiculos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SistemaDeTransitoMunicipal.Vistas"
        mc:Ignorable="d"
        Title="Gestionar Vehiculos" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dg_vehiculosRegistrados" HorizontalAlignment="Left" Height="260" Margin="30,95,0,0" VerticalAlignment="Top" Width="730"/>
        <Button x:Name="btn_registrar" Content="Registrar" HorizontalAlignment="Left" Margin="640,45,0,0" VerticalAlignment="Top" Width="120" Height="30" Background="#FF75F654" FontWeight="Bold" Click="btn_registrar_Click"/>
        <Button x:Name="btn_cancelar" Content="Cancelar" HorizontalAlignment="Left" Margin="660,370,0,0" VerticalAlignment="Top" Width="100" Height="30" Background="#FFF72E15" FontWeight="Bold" Click="btn_cancelar_Click"/>

    </Grid>
</Window>
